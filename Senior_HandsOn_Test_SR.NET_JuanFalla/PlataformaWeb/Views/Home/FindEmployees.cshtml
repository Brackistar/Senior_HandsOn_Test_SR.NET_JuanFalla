
@{
    ViewBag.Title = "Buscar Información de empleados";
}
<div class="row">
    <div class="col-xs-1 col-sm-10 p-sm-1">
        <h2>@ViewBag.Title</h2>
    </div>
</div>
<div class="row">
    <div class="col-xs-12 col-sm-10 p-sm-1">
        <div class="row">
            <div class="col-xs-12 col-sm-10">
                @Html.Editor("Name")
            </div>
            <div class="col-xs-12 col-sm-2 float-right">
                <button type="button" id="SearchEmployees" data-container="Name" data-target="EmployeesSpace"></button>
            </div>
        </div>
    </div>
</div>
<div id="EmployeesSpace" class="row">

</div>

